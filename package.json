{
  "name": "raspberry-piscord",
  "version": "1.0.0",
  "description": "",
  "main": "bot.js",
  "scripts": {
    "precompile": "mkdir -p dist/src",
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "start": "rm cache-store.json; touch cache-store.json; concurrently \"npm run precompile\" \"npm run compile\" \"node ./dist/src/bot.js\"",
    "dev": "export NODE_ENV=dev; concurrently \"npm run precompile\" \"npm run compile:watch\" \"nodemon --inspect --enable-source-maps dist/src/bot.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelmaysonet74/raspberry-piscord.git"
  },
  "author": "Michael Maysonet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaelmaysonet74/raspberry-piscord/issues"
  },
  "homepage": "https://github.com/michaelmaysonet74/raspberry-piscord#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.8",
    "concurrently": "^7.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1",
    "typescript": "^4.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=15.10"
  }
}